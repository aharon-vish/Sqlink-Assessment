    "react-i18next": "^13.5.0",
    "styled-components": "^6.1.19",
    "uuid": "^9.0.1",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/addon-onboarding": "^9.1.2",
    "@storybook/react-vite": "^9.1.2",
